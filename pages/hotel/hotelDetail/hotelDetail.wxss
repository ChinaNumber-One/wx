.headBox{
  position: relative;
}
.headImg{
  width: 100%;
  height: 360rpx;
}
.noImg{
  width: 100%;
  height: 360rpx;
  text-align: center;
  color: #999;
  background: #f1f1f1;
}
.noImg_title{
  margin-top: 20px;
  font-size: 30px;
  font-weight: 900;
}
.noImg_desc {
  font-size: 16x;
}
.hotelDesc {
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.hotelName {
  font-size: 16px;
  width: 100%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.build, .tel {
  font-size: 12px;
}
.headerInfo {
  position: absolute;
  left: 20rpx;
  bottom: 40rpx;
  color:#fff;
  background-color: rgba(0, 0, 0, .7); 
  padding: 5px 10px;
  border-radius: 5px;
  box-sizing: border-box;
   width: 70%;
}
.imgNums {
  float:right;
  font-size: 12px;
  position: absolute;
  right: 20rpx; 
  bottom: 20rpx;
  color: #fff;
  display: flex;
  align-items: center;
}
.icon-tupian {
  font-weight: 900;
}
.nums {
  margin: 0 2px;
}
.hotelTheme {
   display: flex;
  justify-content: space-around; 
   align-items: baseline; 
  text-align: center;
  flex-wrap: wrap;
}
.themeItem {
  background-color: #ffbf1c;
  line-height: 30px;
  color: #fff;
  border-radius: 5px;
  width: 28%;
  margin-bottom: 5px;
}
.position{
  color: #999;
  font-size: 12px;
}
.hotelPosition {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10px;
}
.positionInfo {
  width: 60%;
}
.hotelAddr {
  font-size: 18px;
  width: 100%;
  overflow : hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.mapBtn{
  color: #2a93ff;
  font-size: 12px;
  line-height: 16px;
  display: flex;
  align-items: center;
}
.icon-arrow-right {
  font-size: 18px;
  line-height: 18px;
  color: #999;
}

.advanceNote{
  font-size: 10px;
  color: #999;
  padding: 0 10px;
}
.facilityItem {
  display: inline-block;
  padding: 5px 10px;
  margin: 0 5px;
  background: #f5f5f5;
  border-radius: 5px;
  font-size: 12px;
  color: #666;
}
.activeFacility, .hotelIntro {
  padding: 10px;
}
.hotelIntroText {
  font-size: 12px;
  text-indent: 24px;
  color: #999;
}
.timeBox {
  display: flex;
  align-items: center;
  height: 120rpx;
}
.startTime, .endTime {
  text-align: center;
  width: 40%;
}
.totalDays{
  width: 20%;
  text-align: center;
}
.timeDate, .daysTimes{
  font-size: 16px;
  color: #2a93ff;
}
.daysTimes{
  font-size: 14px;
  color: #2a93ff;
  padding: 0 3px;
}
.roomItem{
  position: relative;
  padding: 10rpx 0;
  margin-bottom: 10rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;

}
.roomItem::after{
  position: absolute;
  bottom: 0;
  content: '';
  width: 100%;
  border-bottom:1px solid #eee;
  transform: scaleY(0.5)
}
.roomImg{
    height: 120px;
    width: 150px;
    display: block;
}
.itemLeft{
  padding-left: 40rpx;
}
.itemRight{
  margin-right: 30px;
}
.roomName{
  font-size: 16px;
}
.breakfast, .windows, .bedType{
  font-size: 12px;
  line-height: 12px;
  padding: 0 10rpx;
}
.bedType{
  padding-left: 0px;
}
.breakfast{
  border-right: 1px solid #999;
  border-left: 1px solid #999
}
.bookingTime{
  font-size: 12px;
  color: #0fbb6c;
}
.itemRight{
  text-align: center;
  font-size: 12px;
}
.booking{
  border: 1px solid #ffbf1c;
  border-radius: 5px;
  overflow: hidden;
}
.booking .phoneBooking{
  background-color: #ff9c01;
  padding: 6rpx 16rpx;
  color: #fff;
}
.booking .payType {
  background: #fff;
  color: #ff9c01;
}
.noBooking{
  border: 1px solid #bababa;
  border-radius: 5px;
  overflow: hidden;
}
.noBooking .phoneBooking{
  background-color: #bababa;
  padding: 6rpx 16rpx;
  color: #fff;
  min-width:40px;
}
.noBooking .payType {
  background: #fff;
  color: #bababa;
}
.minPrice{
  color: #fa791a;
}
.priceNum {
  font-size: 20px;
  margin: 0 10rpx;
}
.priceText{
  font-size: 14px;
}
.roomInfo{
  position: fixed;
  top: 0;
  background: rgba(0, 0, 0, .5);
  z-index: 100;
}
.roomBox{
  background-color: #fff;
  position: absolute;
}
.cancelBtn{
  position: absolute;
  right: 10px;
  top: 10px;
}
.roomBoxName {
  text-align: center;
  position: relative;
  line-height: 40px;
  background: #fff;
}
.swiper{
  height: 350rpx;
}
.imgBox{
  width: 100%;
  display: inline-block;
  position: relative;
}
.roomImg{
   width: 100%; 
   height: 350rpx; 
}
.imgNum{
  position: absolute;
  bottom: 10px;
  right: 10px;
  background: rgba(0, 0, 0, .8);
  color: #fff;
  padding: 2px 10px;
}
.facilities{
  padding: 16px;
}
.facilitiesItem{
  display: inline-block;
  width: 50%;
}
.facilities .title{
  color: #999;
  margin-right: 10px;
}
.moreFacilities{
  font-size: 12px;
  color: #0fbb6c;
}
.icon-icon-arrow-bottom2, .icon-jiantoushang{
  font-size: 10px;
  color: #0fbb6c;
}
.elseFacilities{
  font-size: 10px;
  padding: 0 2px;
}
.bottomBtn{
  position: fixed;
  bottom: 0;
  height: 50px;
  display: flex;
  width: 100%;
}
.bottomBtnLeft{
  width: 50%;
  background: #fff;
  text-align: center;
  line-height: 50px;
  font-weight: 600px;
  color: #fa791a;
}
.bottomBtnRight{
  width:50%;
  background: #ffbf1c;
  color: #fff;
  text-align: center;
  line-height: 50px;
  font-size: 20px;
  font-weight: 900;
}
.rules{
  padding: 0 16px 10px;
}
.rulesItem{
  font-size: 12px;
  margin-bottom: 10px;
}