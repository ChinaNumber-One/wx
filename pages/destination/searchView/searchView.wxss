.container {
  width: 100%;
}
.inputBox {
  display: flex;
  align-items: center; 
  height: 30px;
  position: fixed;
  width: 100%;
  background: #fff;
  padding: 10px 0;
  z-index: 2;
}
.input{
  flex: 1;
  background: #eee;
  height:60rpx;
  margin-left: 20px;
  border-radius: 5px;
  padding-left: 5px;
}
.input-placeholder {
  text-indent: 1.5rem;
}
.cansole {
  width: 16%;
  font-size: 16px;
  text-align: center;
  line-height: 60rpx;
}
.searchIcon {
  position: absolute;
  left: 30px;
  color: #000;
  z-index: 2;
}
.viewList{
  padding: 5px;
}
.viewItem{
  display: flex;
  margin-bottom: 5px;
  height: 120px;
  width: 100%;
}
.viewSmallImg{
  height: 120px;
  width:130px;
}
.viewDesc{
  flex: 1;
  height: 100%;
  box-sizing: border-box;
  padding: 5px;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.viewSummary{
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
  
}
.viewSummary{
  text-indent: 24px;
}
.viewAddress{
  font-size: 12px;
  color: #999;
  white-space: nowrap;
  max-width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
}
.viewName {
  text-align: center;
  font-size: 16px;
  font-weight: 600;
  white-space: nowrap;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
}
.icon-jingdian2{
  font-size: 22px!important;
}
.scrollView {
  margin-top: 50px;
}
.noData {
  font-size: 22px;
  font-weight: 600;
  line-height: 50px;
  text-align: center;
  margin-top: 50px;
}

.pagesContent {
  width: 100%;
  display: flex;
  justify-content: space-around;
  position: fixed;
  bottom: 0;
}
.pickerBtn {
  padding: 0 5px;
  margin: 0 5px;
  text-align: center;
  background: #f5f5f5;
}
.pagesContent {
  height: 30px;
}
.pagesContent{
  line-height: 30px;
}